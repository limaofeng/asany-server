type Query {
{{~#each queries}}
    {{name}}: {{returnType}}
{{~/each}}
}
type Mutation {
{{~#each mutations}}
    {{name}}: {{returnType}}
{{~/each}}
}

{{~#each types}}
type {{name}} {
    {{~#idField}}
    {{name}}: {{graphQLType}}
    {{~/idField}}
    {{~#each fields}}
    {{name}}: {{graphQLType}}
    {{~/each}}
}
{{~/each}}