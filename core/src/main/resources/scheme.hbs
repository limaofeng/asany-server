type Query {
{{~#each queries}}
    {{~#notNull name}}
    """
    {{{name}}}
    """
    {{~/notNull}}
    {{code}}{{#isArg}}(
    {{~#arguments}}
        {{~#notNull description}}
        """
        {{{description}}}
        """
        {{~/notNull}}
        {{name}}: {{type}}{{#required}}!{{/required}}{{#notNull defaultValue}} = {{defaultValue}} {{/notNull}}{{#if @last}}{{else}}, {{/if}}
    {{~/arguments}}
    ){{/isArg}}: {{returnType}}
{{~/each}}
}
type Mutation {
{{~#each mutations}}
    {{~#notNull name}}
    """
    {{{name}}}
    """
    {{~/notNull}}
    {{code}}{{#isArg}}(
    {{~#arguments}}
        {{~#notNull description}}
        """
        {{{description}}}
        """
        {{~/notNull}}
        {{name}}: {{type}}{{#required}}!{{/required}}{{#notNull defaultValue}} = {{defaultValue}} {{/notNull}}{{#if @last}}{{else}}, {{/if}}
    {{~/arguments}}
    ){{/isArg}}: {{returnType}}
{{~/each}}
}

{{~#each types}}
type {{code}} {
    {{~#idField}}
    {{~#notNull name}}
    """
    {{{name}}}
    """
    {{~/notNull}}
    {{code}}: {{type}}
    {{~/idField}}
    {{~#each fields}}
    {{~#notNull name}}
    """
    {{{name}}}
    """
    {{~/notNull}}
    {{code}}: {{type}}
    {{~/each}}
}
{{~/each}}

{{~#each inputTypes}}
input {{code}} {
    {{~#each fields}}
    {{~#notNull name}}
    """
    {{{name}}}
    """
    {{~/notNull}}
    {{code}}: {{type}}
    {{~/each}}
}
{{~/each}}

