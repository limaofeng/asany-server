<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="{{className}}" entity-name="{{code}}" table="{{databaseTableName}}">
        {{~#idField}}
        <id name="{{code}}" type="{{hibernateType}}" length="64" unsaved-value="null">
            <generator class="fantasy-sequence"/>
        </id>
        {{~/idField}}
        {{~#each fields}}
        <property type="{{hibernateType}}" name="{{code}}" column="{{databaseColumnName}}" {{#nonInsertable}}insert="false"{{/nonInsertable}} {{#nonUpdatable}}update="false"{{/nonUpdatable}}/>
        {{~/each}}
    </class>
</hibernate-mapping>